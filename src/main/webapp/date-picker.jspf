<div class="date-picker">
    <form method="GET" action="transactions">
        Search:
        <input id="from-date" name="from-date" type="date" placeholder="yyyy-mm-dd" value="${from}">
        <input id="to-date" name="to-date" type="date" placeholder="yyyy-mm-dd" value="${to}">
        <input id="date_submit" value="Search" type="submit"> 
    </form>
</div>	